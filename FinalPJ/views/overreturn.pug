doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet', href='/stylesheets/overreturn.css')
    title overreturn
  
  body 
    .container
      .main
        .user 
          h1 ชื่อ: #{nameUser.name}
          h1 ฝ่าย: นักศึกษา

        .navbar
          a(href="/index") ยืม/คืน
          a(href="/ustorage") คลังอุปกรณ์
          a(href="/overreturn") ค้างการคืน

        .logout
          a(href="/logout") Logout 


      .page 

        .storage
          h1 ค้างการคืน

        .title 
          //- a1 ประเภท
          a1 อุปกรณ์
          a1 จำนวน

        .inpageStorage 
          
            //- h2 รูป
            //- h2 ชื่อ
            //- h2 จำนวน
            each store in listStorage
              if store.Nameuser === nameUser.name
                .equipmentData 
                  .nameEquipment
                    p1 #{store.name}
                  .amountEquipment
                    p1 #{store.amount}